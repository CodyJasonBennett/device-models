{
  "name": "device-models",
  "version": "1.5.0",
  "homepage": "https://devicemodels.com",
  "description": "A Figma plugin to create mockups with 3D device models.",
  "repository": "https://github.com/CodyJasonBennett/device-models.git",
  "author": "Cody Bennett <hi@codyb.co>",
  "license": "MIT",
  "private": true,
  "devDependencies": {
    "@babel/core": "^7.12.13",
    "@babel/preset-react": "^7.12.13",
    "@storybook/addon-a11y": "^6.1.16",
    "@storybook/addon-actions": "^6.1.16",
    "@storybook/addon-controls": "^6.1.16",
    "@storybook/preset-create-react-app": "^3.1.5",
    "@storybook/react": "^6.1.16",
    "@testing-library/jest-dom": "^5.11.9",
    "@testing-library/react": "^11.2.5",
    "@testing-library/user-event": "^12.6.3",
    "babel-loader": "8.1.0",
    "css-loader": "^5.0.1",
    "enzyme": "^3.11.0",
    "enzyme-adapter-react-16": "^1.15.5",
    "file-loader": "^6.2.0",
    "html-webpack-inline-source-plugin": "1.0.0-beta.2",
    "html-webpack-plugin": "^5.0.0",
    "prettier": "^2.2.1",
    "react-scripts-postcss": "4.0.0-mod1",
    "react-snap": "1.23.0",
    "source-map-explorer": "^2.5.2",
    "style-loader": "^2.0.0",
    "stylelint": "^13.8.0",
    "stylelint-config-recommended": "^3.0.0",
    "url-loader": "^4.1.1",
    "webpack": "4.44.2",
    "webpack-cli": "^4.5.0"
  },
  "dependencies": {
    "classnames": "^2.2.6",
    "popmotion": "^8.7.6",
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
    "react-helmet": "^6.1.0",
    "react-router-dom": "5.2.0",
    "react-transition-group": "^4.4.1",
    "three": "^0.125.2"
  },
  "scripts": {
    "start": "set PORT=80 && react-scripts start",
    "start-plugin": "set NODE_ENV=development&&webpack --watch --mode development",
    "build": "react-scripts build",
    "build-plugin": "set NODE_ENV=production&&webpack --mode production",
    "build-storybook": "build-storybook -o build-storybook",
    "postbuild": "react-snap",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "storybook": "start-storybook -p 9009 -s public",
    "analyze": "source-map-explorer 'build/static/js/*.js'"
  },
  "reactSnap": {
    "puppeteerArgs": [
      "--no-sandbox",
      "--disable-setuid-sandbox"
    ],
    "skipThirdPartyRequests": true,
    "headless": true,
    "crawl": true
  },
  "browserslist": {
    "production": [
      ">10%",
      "not dead",
      "not ie 11",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "proxy": "https://devicemodels.com"
}
